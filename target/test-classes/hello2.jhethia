#
# Define the Fibonacci number of the given element index
#

kaam fibonacci_number [ n ]
    agar n < 2
        return n
    khatam
    return fibonacci_number [ n - 1 ] + fibonacci_number [ n - 2 ]
khatam

lijiye_baapuji index_number
dekhiye_baapuji "fibonacci number is " + fibonacci_number [ index_number ]